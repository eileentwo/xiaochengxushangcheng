/**app.wxss**/
.usermask{
  position: fixed;
  z-index: 99;
  background: transparent;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}
.phonemask{
  z-index: 999
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  overflow-y: scroll;
} 
.scrollBox{
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
/* 隐藏滚动条 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;

}
/*flexbox盒模型  */
.flexbox{
  display: flex;
  flex-direction: row;
}
/* 圆角背景盒子 */
.radiusbox {
  width: 662rpx;
  padding: 0 20rpx 20rpx;
  margin: 0 auto 34rpx;
  background-color: #fff;
  border-radius: 15rpx;

}

.spaceBetween{
  justify-content: space-between
}
/* 下边框为0 */
.nobottomB{
  border-bottom:0;
}

.ellipsis{
  overflow: hidden;
  text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  white-space: nowrap;
}
.navigator-hover {
  background-color:transparent;

}

.nomore-tip image{
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
 
}
.help{
  position: fixed;
  right: 30rpx;
  bottom: 200rpx;
  width: 90rpx;
  height: 90rpx;
  z-index: 999;
}
.help image{
  width: 100%;
  height: 100%;
  position: absolute;
  
}
.help button{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: transparent;
  z-index: 1;
}